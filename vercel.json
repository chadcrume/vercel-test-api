{
    "functions": {
        "api/deck/[deckName].js": {
            "includeFiles": "api/deck/deckData/**"
        },
        "api/[gitUser]/[repo].js": {
            "includeFiles": "api/[gitUser]/repos/**"
        }
    }
    ,
    "rewrites": [
        { "source": "/(api|ccc)/:gitUser/:repo/:path*", "destination": "/api/[gitUser]/[repo].js" }
    ]
}